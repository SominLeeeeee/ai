   "source": [
    "import numpy as np\n",
    "\n",
    "A = np.array([[0,0,-2],[1,2,1],[1,0,3]])\n",
    "\n",
    "# 대각행렬을 타이핑하기 어려우므로 B라고 하겠다\n",
    "# A = PBP^-1\n",
    "# P^-1AP = B\n",
    "\n",
    "P = np.array([[-1,0,-2],[0,1,1],[1,0,1]])\n",
    "invP = np.linalg.inv(P)\n",
    "\n",
    "B = invP.dot(A.dot(P))\n",
    "print(B)\n",
    "\n",
    "# 대각행렬은 고윳값을 대각선에 배치한 행렬이다"
   ]
